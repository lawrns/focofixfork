  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/unit/integration/",
    "test:contract": "vitest run tests/contracts/",
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:ui": "playwright test tests/e2e/ --ui",
    "test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:contract && npm run test:e2e",
    "test:comprehensive": "npm run test:unit && npm run test:contract && npm run test:e2e && npm run test:accessibility && npm run test:performance && npm run test:security",
    "test:accessibility:full": "playwright test tests/accessibility/",
    "test:accessibility:report": "playwright test tests/accessibility/ --reporter=html",
    "test:performance": "playwright test tests/performance/",
    "test:performance:report": "playwright test tests/performance/ --reporter=json",
    "test:security": "playwright test tests/security/",
    "test:security:report": "playwright test tests/security/ --reporter=html",
    "test:visual": "playwright test tests/visual/",
    "test:load": "k6 run tests/load/",
    "test:smoke": "playwright test tests/smoke/",
    "test:api:production": "bash run-api-tests.sh",
    "test:components": "vitest run tests/unit/components/",
    "test:services": "vitest run tests/unit/services/",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=json",
    "test:report": "node scripts/generate-test-report.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:migrate:test": "supabase db push --schema test",
    "db:seed:test": "supabase db seed --schema test",
    "start:test": "NODE_ENV=test next start",
    "build:test": "NODE_ENV=test next build",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "analyze": "ANALYZE=true next build"
  },
