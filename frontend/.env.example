# Foco Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
# Get these from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# For migration from Crico (optional)
# CRICO_SUPABASE_URL=https://your-crico-project.supabase.co
# CRICO_SUPABASE_ANON_KEY=your-crico-anon-key-here

# Ollama Configuration (optional)
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama2

# Application Configuration
NEXT_PUBLIC_APP_NAME=Foco
NEXT_PUBLIC_APP_URL=https://your-netlify-site.netlify.app

# Analytics (optional)
# NEXT_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# Error Tracking (optional)
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Development
NODE_ENV=development

# Netlify-specific (automatically set)
# BRANCH=your-branch-name
# CONTEXT=production|deploy-preview|branch-deploy
# SITE_NAME=your-site-name
